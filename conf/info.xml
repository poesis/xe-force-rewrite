<?xml version="1.0" encoding="UTF-8"?>
<addon version="0.2">
    <title xml:lang="ko">짧은 주소로 리다이렉트</title>
    <title xml:lang="en">Redirect to a short address</title>
    <description xml:lang="ko">긴 주소로 접속하면 짧은 주소로 리다이렉트합니다. SEO에 도움이 될 수 있습니다.</description>
    <description xml:lang="en">Accessing a long address will be redirect to a short address. It can help with SEO.</description>
    <version>2.0.0</version>
    <date>2018-09-08</date>
	<author link="https://www.poesis.org">
		<name xml:lang="ko">포에시스</name>
		<name xml:lang="en">poesis</name>
	</author>
	<extra_vars>
		<var name="remove_page" type="select">
			<title xml:lang="ko">페이지 변수 제거</title>
			<title xml:lang="en">Remove Page Variables</title>
			<options value="N">
				<title xml:lang="ko">아니오</title>
				<title xml:lang="en">N</title>
			</options>
			<options value="Y">
				<title xml:lang="ko">예</title>
				<title xml:lang="en">Y</title>
			</options>
		</var>
		<var name="remove_search" type="select">
			<title xml:lang="ko">검색어 변수 제거</title>
			<title xml:lang="en">Remove Search Query Variable</title>
			<options value="N">
				<title xml:lang="ko">아니오</title>
				<title xml:lang="en">N</title>
			</options>
			<options value="Y">
				<title xml:lang="ko">예</title>
				<title xml:lang="en">Y</title>
			</options>
		</var>
		<var name="remove_lang" type="select">
			<title xml:lang="ko">다국어 변수 제거</title>
			<title xml:lang="en">Remove Language Variable</title>
			<options value="N">
				<title xml:lang="ko">아니오</title>
				<title xml:lang="en">N</title>
			</options>
			<options value="Y">
				<title xml:lang="ko">예</title>
				<title xml:lang="en">Y</title>
			</options>
		</var>
		<var name="remove_other" type="select">
			<title xml:lang="ko">그 밖의 불필요한 변수 제거</title>
			<title xml:lang="en">Remove Other Unnecessary Variables</title>
			<options value="N">
				<title xml:lang="ko">아니오</title>
				<title xml:lang="en">N</title>
			</options>
			<options value="Y">
				<title xml:lang="ko">예</title>
				<title xml:lang="en">Y</title>
			</options>
		</var>
	</extra_vars>
</addon>
